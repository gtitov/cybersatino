{
  "version": 8,
  "name": "cyberSatino",
  "metadata": {
    "maputnik:license": "https://github.com/maputnik/osm-liberty/blob/gh-pages/LICENSE.md",
    "maputnik:renderer": "mbgljs",
    "openmaptiles:version": "3.x"
  },
  "center": [
    36.35563,
    55.21056
  ],
  "zoom": 13,
  "sources": {
    "satino": {
      "type": "vector",
      "tiles": [
        "http://localhost:5500/satino/{z}/{x}/{y}.pbf"
      ]
    }
  },
  "layers": [
    {
      "id": "back",
      "type": "background"
    },
    {
      "id": "landUse",
      "type": "fill",
      "source": "satino",
      "source-layer": "landUse",
      "filter": [
        "all"
      ],
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": [
          "match",
          [
            "get",
            "Land_Type"
          ],
          "Леса",
          "#160016",
          "Луга",
          "#0F000F",
          "Пашни",
          "#0F000F",
          "Заболоченные земли",
          "#000022",
          "Территории населенных пунктов",
          "#191919",
          "#000000"
        ],
        "fill-opacity": 1
      }
    },
    {
      "id": "buildingsPseudo",
      "type": "fill",
      "source": "satino",
      "source-layer": "buildingsPseudo",
      "minzoom": 12,
      "maxzoom": 14,
      "paint": {
        "fill-color": "#333333"
      }
    },
    {
      "id": "waterPolygons",
      "type": "fill",
      "source": "satino",
      "source-layer": "waterPolygons",
      "paint": {
        "fill-color": "#000011",
        "fill-antialias": false
      }
    },
    {
      "id": "waterOutline",
      "type": "line",
      "source": "satino",
      "source-layer": "waterPolygons",
      "paint": {
        "line-color": "rgba(82, 84, 250, 1)",
        "line-width": 1
      }
    },
    {
      "id": "waterLines",
      "type": "line",
      "source": "satino",
      "source-layer": "waterLines",
      "paint": {
        "line-color": "rgba(82, 84, 250, 1)",
        "line-width": 1
      }
    },
    {
      "id": "contours",
      "type": "line",
      "source": "satino",
      "source-layer": "contoursBasic",
      "paint": {
        "line-width": [
          "case",
          [
            "==",
            [
              "%",
              [
                "get",
                "Value_"
              ],
              10
            ],
            0
          ],
          1,
          0.5
        ],
        "line-color": "#004D0A"
      }
    },
    {
      "id": "roadsBlur",
      "type": "line",
      "source": "satino",
      "source-layer": "roads",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(255, 124, 0, 0.6)",
        "line-width": 5,
        "line-blur": 5
      }
    },
    {
      "id": "roads",
      "type": "line",
      "source": "satino",
      "source-layer": "roads",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(255, 124, 0, 0.6)",
        "line-width": 1,
        "line-blur": 1
      }
    },
    {
      "id": "buildingsPseudo3d",
      "source": "satino",
      "source-layer": "buildingsPseudo",
      "type": "fill-extrusion",
      "minzoom": 13,
      "paint": {
        "fill-extrusion-color": "#444444",
        "fill-extrusion-height": [
          "interpolate", ["linear"], ["zoom"],
          13.5, ["get", "Height"],
          13.7, ["*", ["get", "Height"], 10]
        ],
        "fill-extrusion-base": 0,
        "fill-extrusion-opacity": 1
      }
    },
    {
      "id": "frameBlur2",
      "type": "line",
      "source": "satino",
      "source-layer": "frame",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#d42cca",
        "line-width": 15,
        "line-blur": 15
      }
    },
    {
      "id": "frameBlur1",
      "type": "line",
      "source": "satino",
      "source-layer": "frame",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#ffd5ff",
        "line-width": 8,
        "line-blur": 8
      }
    },
    {
      "id": "frame",
      "type": "line",
      "source": "satino",
      "source-layer": "frame",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#ffffff",
        "line-width": 1
      }
    }
  ],
  "id": "cybersatino"
}